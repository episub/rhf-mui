{
  "name": "@episub/rhf-mui",
  "version": "1.0.0",
  "description": "Glue components to help intergrating react-hook-forms with mui",
  "license": "MIT",
  "author": "myrovh",
  "main": "index.js",
  "scripts": {
    "b": "rimraf build && babel src -d build",
    "build": "yarn build:cjs && yarn build:esm && yarn build:es && yarn build:types",
    "build:cjs": "node ./scripts/build cjs",
    "build:es": "node ./scripts/build es",
    "build:esm": "node ./scripts/build esm",
    "build:types": "tsc -p tsconfig.build.json",
    "format": "prettier --write .",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@types/react-window": "^1.8.2",
    "autosuggest-highlight": "^3.1.1",
    "mdi-material-ui": "^6.18.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.8.2",
    "react-window": "^1.8.5",
    "ts-toolbelt": "^8.0.5"
  },
  "devDependencies": {
    "@types/autosuggest-highlight": "^3.1.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.9.0",
    "eslint-config-bigbrain": "^1.1.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "prettier": "^2.1.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-packagejson": "^2.2.5",
    "typescript": "^4.0.2"
  }
}
